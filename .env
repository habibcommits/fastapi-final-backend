# .env.example
APP_NAME=PDF Processing Service
APP_VERSION=1.0.0
DEBUG=false
LOG_LEVEL=INFO

# File Processing
MAX_FILE_SIZE_MB=50
MAX_FILES_COUNT=20
ALLOWED_IMAGE_TYPES=image/jpeg,image/png,image/webp,image/tiff,image/bmp
TEMP_DIR=/tmp/pdf-service

# Rate Limiting
RATE_LIMIT_PER_MINUTE=30

# Server
HOST=0.0.0.0
PORT=8000
WORKERS=4
